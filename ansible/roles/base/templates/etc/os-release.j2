{% if ansible_lsb['id'] is defined and ansible_lsb['id'] == 'ManjaroLinux' %}
NAME="Manjaro Linux"
ID=manjaro
ID_LIKE=arch
BUILD_ID=rolling
PRETTY_NAME="Manjaro Linux"
ANSI_COLOR="32;1;24;144;200"
